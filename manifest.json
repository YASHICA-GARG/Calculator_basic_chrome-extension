{
    "manifest_version" : 2,
    "name" : "Pro Calculator",
    "version" : "1.0",
    "description" : "Get your calculation instantly",
    "icons" : {
        "128" : "img/icon128.png",
        "48" : "img/icon48.png",
        "16" : "img/icon16.png"
    },
    "browser_action" : {
        "default_icon" : "img/icon16.png",
        "default_popup" : "main.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}